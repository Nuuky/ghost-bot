{
  "install": {
    "include": [
      "^package\\.json$",
      "^\\.env$"
    ]
  },
  "restart": {
    "exclude": [
      "^public/",
      "^dist/",
      "^json/",
      "^cmds/",
      "^global/",
      "^index\\.js$"
    ],
    "include": [
      "\\.js$",
      "\\.json"
    ]
  },
  "throttle": 30000
}